[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[remote "origin"]
	url = git@github.com:Perseverancia-company/sub.gitconfig.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "main"]
	remote = origin
	merge = refs/heads/main
[submodule "sqlite3_utils"]
	url = git@github.com:Perseverancia-company/sub.sqlite3-utils.git
	active = true
[alias]
	supdate = submodule update --remote --init --recursive --merge
[submodule "dotenv5"]
	url = git@github.com:Perseverancia-company/sub.dotenv5.git
	active = true
[submodule "process_utils"]
	url = git@github.com:Perseverancia-company/sub.process-utils.git
	active = true